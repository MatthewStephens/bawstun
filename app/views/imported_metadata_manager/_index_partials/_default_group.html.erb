<table class="table table-striped dashboard">
  <caption class="accessible-hidden">Listing of files you have deposited in <%= t('sufia.product_name') %></caption>
<% unless params[:display_type] == 'list' %>
  <thead> 
  <tr>
    <th class="sm"><%= render :partial => 'batch_edits/check_all' %></th>
    <th>&nbsp;</th>
    <th class="center">Series Title</th>
    <th class="center">Program Title</th>
    <th class="center">Item Title</th>
    <th class="center">Drive Name</th>
    <th class="center">Folder Name</th>
    <th class="center">Action</th>
  </tr>
  </thead> 
  <% end %>
  <tbody> 
  <% docs.each_with_index do |document,counter| %>
    <% if document["active_fedora_model_ssi"] == "GenericFile" %>
      <%= render :partial => 'dashboard/_index_partials/list_files', :locals => {:document => document, :counter => counter} %>
    <% elsif document["active_fedora_model_ssi"] == "ImportedMetadata" %>
      <%#= document.inspect%>
      <%= render :partial => 'imported_metadata_manager/_index_partials/imported_metadata', :locals => {:document => document, :counter => counter} %>
    <% else %>
    <% end %>
  <% end %>
  </tbody> 
</table>